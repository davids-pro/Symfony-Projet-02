{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
	<div class="row pt-5">
		{% for categorie in categories %}
			<div class="card bg-dark text-light col-4">
				<div class="card-body">
					<h3>
						<b class="text-danger">{{categorie.title}}</b>
					</h3>
				</div>
			</div>
		{% endfor %}
	</div>
{% endblock %}
